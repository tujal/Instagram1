<% like = current_user.likes.find_by(likable: likable) %>
<% if like.nil? %>
    <%= button_to likes_path(params:{ like: {likable_id: likable,likable_type: likable.class.to_s}}), class:"bg-white btn btn-link" ,remote: true, data: { turbo: "false" } do %>
            <img src="https://cdn-icons-png.flaticon.com/128/1077/1077035.png", style="width:25px; height:25px"><br>
            <%= likable.likes.count %>
    <% end %>
<% else %>
  <%= button_to like_path(like), method: :delete, class:"bg-white btn btn-link", remote: true, data: { turbo: "false" } do %>
            <img src="https://cdn-icons-png.flaticon.com/128/833/833472.png", style="width:25px; height:25px"><br>
            <%= likable.likes.count %>
   <% end %>
<% end %>
